--// Services & shortâ€‘cuts
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
--heer
local LocalPlayer = Players.LocalPlayer
local PlayerGui = LocalPlayer:WaitForChild("PlayerGui")

--// GUI elements (must already exist)
local AdminGui = PlayerGui:WaitForChild("Spawn")                 -- ScreenGui                 -- Frame with UIListLayout
local SpawnButton = AdminGui:WaitForChild("Button")            -- TextButton

local SpawnRemote = ReplicatedStorage:WaitForChild("Events"):WaitForChild("admincommands"):WaitForChild("SpawnItem")                                         -- userdata reference

SpawnButton.MouseButton1Click:Connect(function()
    SpawnRemote:FireServer(ReplicatedStorage.Loot["Limited Time"].Summer.Godly["Tan Skinwalker"], LocalPlayer, 1)
end)
